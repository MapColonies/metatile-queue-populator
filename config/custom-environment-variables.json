{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "app": {
    "projectName": "APP_PROJECT_NAME",
    "enableRequestQueueHandling": {
      "__name": "APP_ENABLE_REQUEST_QUEUE_HANDLING",
      "__format": "boolean"
    },
    "requestQueueCheckIntervalSec": {
      "__name": "APP_REQUEST_QUEUE_CHECK_INTERVAL_SEC",
      "__format": "number"
    },
    "tilesBatchSize": {
      "__name": "APP_TILES_BATCH_SIZE",
      "__format": "number"
    },
    "metatileSize": {
      "__name": "APP_METATILE_SIZE",
      "__format": "number"
    }
  },
  "db": {
    "host": "DB_HOST",
    "port": {
      "__name": "DB_PORT",
      "__format": "number"
    },
    "user": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "schema": "DB_SCHEMA",
    "enableSslAuth": {
      "__name": "DB_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "DB_CA_PATH",
      "key": "DB_KEY_PATH",
      "cert": "DB_CERT_PATH"
    },
    "database": "DB_NAME",
    "expireInSeconds": {
      "__name": "DB_JOBS_EXPIRE_IN_SECONDS",
      "__format": "number"
    },
    "retryBackoff": {
      "__name": "DB_JOBS_RETRY_BACKOFF",
      "__format": "boolean"
    },
    "retryLimit": {
      "__name": "DB_JOBS_RETRY_LIMIT",
      "__format": "number"
    },
    "retryDelay": {
      "__name": "DB_JOBS_RETRY_DELAY",
      "__format": "number"
    },
    "retentionHours": {
      "__name": "DB_JOBS_RETENTION_HOURS",
      "__format": "number"
    }
  }
}
